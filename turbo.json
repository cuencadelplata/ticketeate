{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", ".tsbuildinfo"],
      "cache": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$", "src/**/*.ts", "src/**/*.tsx", "!**/.next/**", "!**/dist/**", "!**/build/**", "!**/node_modules/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "inputs": ["$TURBO_DEFAULT$", "src/**/*.ts", "src/**/*.tsx", "test/**/*.ts", "test/**/*.tsx", "**/*.test.ts", "**/*.test.tsx", "**/*.spec.ts", "**/*.spec.tsx"],
      "outputs": ["coverage/**", ".nyc_output/**"],
      "cache": true
    },
    "check-types": {
      "dependsOn": ["^build", "^check-types"],
      "cache": true,
      "inputs": ["$TURBO_DEFAULT$", "tsconfig.json", "src/**/*.ts", "src/**/*.tsx"]
    },
    "dev": {
      "cache": true,
      "persistent": true,
      "dependsOn": []
    },
    "redis-service#dev": {
      "cache": false
    },
    "cli#build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "Cargo.toml", "src/**/*.rs"],
      "outputs": ["target/**"],
      "cache": true
    }
  }
}
